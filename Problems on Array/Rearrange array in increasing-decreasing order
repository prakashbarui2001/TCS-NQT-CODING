#include <bits/stdc++.h>
using namespace std;
void getElements(int arr[],int n)
{  
  
       sort(arr,arr+n);
       for(int i=0;i<n/2;++i){
        cout<<arr[i]<<" ";
       }
       for(int i=n-1;i>=n/2;--i){
        cout<<arr[i]<<" ";
       }


}
int main()
{
  #ifndef ONLINE_JUDGE
      freopen("input.txt","r",stdin);
      freopen("output.txt","w",stdout);
      #endif
      
     
      int n;
      cin>>n;
      int arr[n];
      for(int i=0;i<n;++i){
        cin>>arr[i];
      }
      getElements(arr,n);
      }
