

import java.util.*;
import java.lang.*;
import java.io.*;


class Codechef
{
    
	public static void main (String[] args) throws java.lang.Exception
	{
		int arr[]={1,2,3,4,5};
	    int n=arr.length;
	    int sum=0;
	    for(int i=0;i<n;++i){
	        
	        sum+=arr[i];
	    }
	    //cout<<"Average is:"<<sum/n<<endl;
	    System.out.print(sum/n);
		
		
	}
}
