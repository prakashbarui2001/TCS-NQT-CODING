
#include <bits/stdc++.h>
using namespace std;
void Smallestnumber(int arr[],int n){


        int mn=INT_MAX;
        for( int i=0;i<n;++i){

          mn=min(mn,arr[i]);
        }
        cout<<mn<<" ";
}
int main()
{
  #ifndef ONLINE_JUDGE
      freopen("input.txt","r",stdin);
      freopen("output.txt","w",stdout);
      #endif
      
     int n;
     cin>>n;
     int arr[n];
     for(int i=0;i<n;++i)
     {
      cin>>arr[i];
     }
     Smallestnumber(arr,n);
   
     
      return 0;
}
