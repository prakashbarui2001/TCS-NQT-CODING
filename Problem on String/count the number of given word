import java.util.*;
 class Codechef{
    
    static void repetedword(String s){
        int count=1;
        for(int i=0;i<s.length();++i){
            
            if(s.charAt(i)==' '){
                count++;
            }
        }
        
        System.out.println(count);
        
    }
    
    
    public static void main(String []args){
     String s="abcd aabbcc boy";
     repetedword(s);
     
    }
    
}
