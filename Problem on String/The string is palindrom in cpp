
#include <bits/stdc++.h>
using namespace std;
void stringpalindrom(string s){

     int n=s.size();
     string s1=s;
     string s2;
     for(int i=s.size()-1;i>=0;--i){

          s2.push_back(s[i]);
     }
     if(s1==s2){
      cout<<"The string is palindrom."<<endl;
     }
     else{
      cout<<"The string is not palindrom"<<endl;
     }
}
int main()
{
  #ifndef ONLINE_JUDGE
      freopen("input.txt","r",stdin);
      freopen("output.txt","w",stdout);
      #endif
      
    string s;
    cin>>s;
    stringpalindrom(s);

}
